- if (!socialoauth.loggedIn)
  h2 Not Authenticated
  #register
    a(href='/register') Register
  #password-login
    a(href='/login', style='border: 0px') Login with Password
  #qq-login
    a(href='/auth/qq', style='border: 0px') Login with QQ
  #weibo-login
    a(href='/auth/weibo', style='border: 0px') Login with weibo
  #douban-login
    a(href='/auth/douban', style='border: 0px') Login with douban
  #baidu-login
    a(href='/auth/baidu', style='border: 0px') Login with baidu
  #github-login
    a(href='/auth/github', style='border: 0px') github
- else
  h2 Authenticated
  - if (socialoauth.github)
    h3 GitHub User Data
    p= JSON.stringify(socialoauth.github.user)
  - if (socialoauth.qq)
    h3 QQ User Data
    p= JSON.stringify(socialoauth.qq.user)
  - if (socialoauth.weibo)
    h3 weibo User Data
    p= JSON.stringify(socialoauth.weibo.user)
  - if (socialoauth.douban)
    h3 douban User Data
    p= JSON.stringify(socialoauth.douban.user)
  - if (socialoauth.baidu)
    h3 baidu User Data
    p= JSON.stringify(socialoauth.baidu.user)
  - if (socialoauth.github)
    h3 GitHub User Data
    p= JSON.stringify(socialoauth.github.user)
  h3
    a(href='/logout') Logout
